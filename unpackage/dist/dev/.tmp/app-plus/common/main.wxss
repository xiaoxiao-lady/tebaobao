/* 通用样式 */
view{font-size:28rpx;}
.grace-padding{padding:2%; width:96%;}
.grace-common-bg{background:#F8F8F8;}
.grace-common-mt{margin-top:15px;}
.grace-common-border{border-top:1px solid #E9E9E9; border-bottom:1px solid #E9E9E9;}
.grace-noborder{border:0 !important;}
@-webkit-keyframes gradient{50%{background-position:100% 0;}}
@keyframes gradient{50%{background-position:100% 0;}}
.grace-gradient-bg{width:100%;
  background: -webkit-linear-gradient(45deg,#5a3694 0,#13bdce 33%,#0094d9 66%,#6fc7b5 100%);
  background: linear-gradient(45deg,#5a3694 0,#13bdce 33%,#0094d9 66%,#6fc7b5 100%);
  background-size: 400%; background-position:0 100%; -webkit-animation: gradient 7.5s ease-in-out infinite; animation: gradient 7.5s ease-in-out infinite; padding:50rpx 0;
}
.grace-gradient-bg view{color:#FFF;}
/* 通用 css 动画 */
@-webkit-keyframes grace-fade-in {0% {opacity: 0.1;}
40% {opacity: 0.5;}100% {opacity: 1;}}
@keyframes grace-fade-in {0% {opacity: 0.1;}
40% {opacity: 0.5;}100% {opacity: 1;}}
.grace-fade-in{-webkit-animation : grace-fade-in 200ms linear;animation : grace-fade-in 200ms linear;}
/* 通用组件声明 */
graceSwiper{width:100%; height:auto;}
graceSlider, graceSpeaker, graceLoading{width:100%;}
/* 文字相关 */
.grace-center{-webkit-box-pack:center;-webkit-justify-content:center;justify-content:center; text-align:center;}
.grace-h1{font-size:80rpx; line-height:1.8em;}
.grace-h2{font-size:60rpx; line-height:1.8em;}
.grace-h3{font-size:45rpx; line-height:1.8em;}
.grace-h4{font-size:32rpx; line-height:1.8em;}
.grace-h5{font-size:30rpx; line-height:1.8em;}
.grace-text{font-size:28rpx; line-height:2.2em;}
.grace-text image{width:100%; margin:20rpx 0;}
.grace-text-small{font-size:24rpx; line-height:1.8em;}
.grace-line-through{text-decoration:line-through;}
.grace-italic{font-style:italic;}
.grace-indent{text-indent:2em;}
.grace-blod{font-weight:700;}
.grace-ellipsis{width:100%; overflow:hidden; white-space:nowrap; text-overflow:ellipsis;}
/* 栅格布局 */
.grace-flex{display:-webkit-box !important;display:-webkit-flex !important;display:flex !important;}
.grace-rows{display:-webkit-box;display:-webkit-flex;display:flex; -webkit-box-orient:horizontal !important; -webkit-box-direction:normal !important; -webkit-flex-direction:row !important; flex-direction:row !important;}
.grace-columns{display:-webkit-box;display:-webkit-flex;display:flex; -webkit-box-orient:vertical !important; -webkit-box-direction:normal !important; -webkit-flex-direction:column !important; flex-direction:column !important;}
.grace-wrap{display:-webkit-box;display:-webkit-flex;display:flex; -webkit-flex-wrap:wrap; flex-wrap:wrap;}
.grace-nowrap{display:-webkit-box;display:-webkit-flex;display:flex; -webkit-flex-wrap:nowrap !important; flex-wrap:nowrap !important;}
.grace-space-between{display:-webkit-box;display:-webkit-flex;display:flex; -webkit-box-pack:justify !important; -webkit-justify-content:space-between !important; justify-content:space-between !important;}
/* 背景颜色 */
.grace-bg-red{background:#F76260;}
.grace-bg-green{background:#00C777;}
.grace-bg-blue{background:#10AEFF;}
.grace-bg-white{background:#FFFFFF;}
/* 滚动区域 */
.grace-scroll-x{width:100%; display:-webkit-box; display:-webkit-flex; display:flex; white-space:nowrap; height:auto; font-size:0;}
.grace-scroll-x .grace-items{width:355rpx; height:200rpx; vertical-align:top; margin:0 10rpx; display:-webkit-inline-box; display:-webkit-inline-flex; display:inline-flex;}
.grace-scroll-y{width:100%; display:-webkit-box; display:-webkit-flex; display:flex; font-size:0; height:200rpx;}
.grace-scroll-y .grace-items{width:100%; height:200rpx;}
/* 数字角标 */
.grace-badge{border-radius:38rpx; height:38rpx; line-height:38rpx; padding:0 13rpx; font-size:22rpx; background:#D1D1D1;}
.grace-badge-red{background:#F76260; color:#FFF !important;}
.grace-badge-green{background:#00C777; color:#FFF !important;}
.grace-badge-blue{background:#10AEFF; color:#FFF !important;}
.grace-badge-yellow{background:#F0AD4E; color:#FFF !important;}
/* 普通列表 */
.grace-list{width:100%; border-top:1px solid #E9E9E9; border-bottom:1px solid #E9E9E9; background:#FFF; padding:5px 0;}
.grace-list .items{width:100%; display:-webkit-box; display:-webkit-flex; display:flex; -webkit-flex-wrap:nowrap; flex-wrap:nowrap;  position:relative;}
.grace-list .items .icons{width:20px; height:20px; line-height:20px; text-align:center; margin:18px 0 15px 15px; -webkit-flex-shrink:0; flex-shrink:0; overflow:hidden; font-size:0;}
.grace-list .items .icons image{width:20px; height:20px;}
.grace-list .items .title{width:100%; margin-left:18px; padding:18px 30px 18px 0; font-size:16px; height:20px; line-height:20px; overflow:hidden; border-bottom:1px solid #E9E9E9; white-space:nowrap; text-overflow:ellipsis;}
.grace-list .title text{font-size:13px; color:#B2B2B2; margin-left:10px; float:right;}
.grace-list > .items:last-child .title {border:none !important;}
.grace-list .items .arrow-right{font-family:"grace-iconfont" !important; font-size:36rpx; font-style:normal; height:56px; line-height:56px; text-align:center; width:30px; position:absolute; z-index:1; right:0; top:0;}
.grace-list .items .arrow-right:before{content:"\E601"; color:#B2B2B2;}
.grace-list .items .icon-r{position:absolute; z-index:1; right:35px; top:0px;}
/* 多格图标 */
.grace-boxes{width:23%; margin:0 1%; padding:10rpx 0;}
.grace-boxes-img{width:70%; margin:0 auto; text-align:center; padding-bottom:10rpx; font-size:0;}
.grace-boxes-img image{width:100%;}
.grace-boxes-text{line-height:2em; text-align:center; font-size:22rpx;}
/* 图文列表 */
.grace-imgitems{display:-webkit-box;display:-webkit-flex;display:flex; width:100%; -webkit-flex-wrap:wrap; flex-wrap:wrap;}
.grace-imgitems .grace-items{width:48%; margin:10rpx 1%; overflow:hidden; font-size:0; position:relative;}
.grace-imgitems image{width:100%;}
.grace-imgitems-text{margin:6rpx 1%; width:98%; font-size:26rpx; height:50rpx; line-height:50rpx;}
.grace-imgitems-more{display:-webkit-box;display:-webkit-flex;display:flex; -webkit-box-pack:justify; -webkit-justify-content:space-between; justify-content:space-between; line-height:46rpx;}
.grace-imgitems-button{width:auto; border:1px solid #00C777; line-height:44rpx; padding:0 20rpx; color:#49A761; font-size:24rpx; border-radius:5rpx;}
.grace-imgitems-tips{display:-webkit-box;display:-webkit-flex;display:flex; -webkit-flex-wrap:nowrap; flex-wrap:nowrap; width:auto; position:absolute; top:10rpx; color:#FFFFFF; height:36rpx; font-size:20rpx; line-height:36rpx; background:#F76260; padding:0 6px;}
.grace-imgitems-tips-green {background: #00C777;}
.grace-imgitems-tips-r{right:0;}
/* 新闻列表 */
.grace-news-list{padding:12rpx 0;}
.grace-news-list > navigator{display:block; width:100%;}
.grace-news-list-items{width:100%; display:-webkit-box; display:-webkit-flex; display:flex; -webkit-flex-wrap:nowrap; flex-wrap:nowrap; position:relative; padding:10px 0; margin:12rpx 0;}
.grace-news-list-img{width:200rpx; -webkit-flex-shrink:0; flex-shrink:0;}
.grace-news-list-title{width:100%; overflow:hidden;}
.grace-news-list-title-main{line-height:1.5em; font-size:32rpx; width:100%;}
.grace-news-list-title-desc{font-size:24rpx; display:block; color:#666; margin-top:12rpx; height:40rpx; line-height:40rpx;}
.grace-news-tips{width:auto; padding:0 12rpx; border-radius:5rpx; overflow:hidden; background:#FF4343; position:absolute; top:0; color:#FFFFFF; height:36rpx; line-height:36rpx; font-size:20rpx;}
.grace-news-tips-l{left:0;}
.grace-news-tips-r{right:0;}
.grace-news-list-info{width:100%; margin-top:10rpx; line-height:36rpx; font-size:24rpx; color:#666; -webkit-box-pack:justify; -webkit-justify-content:space-between; justify-content:space-between;}
.grace-news-list-info view{font-size:24rpx; color:#666;}
.grace-news-list-info text{font-size:24rpx; color:#666; margin-right:10rpx;}
.grace-news-list-img-news{width:100%; -webkit-flex-wrap:wrap; flex-wrap:wrap;}
.grace-news-list-imgs{-webkit-box-pack:justify;-webkit-justify-content:space-between;justify-content:space-between; width:100%; display:-webkit-box; display:-webkit-flex; display:flex; margin:18rpx 0; -webkit-box-align:start; -webkit-align-items:flex-start; align-items:flex-start;}
.grace-news-list-imgs image{width:31.3%; margin:0 1%;}
.grace-news-list-img-big{width:100%; padding:18rpx 0;}
.grace-news-list-img-big image{width:100%;}
.grace-list-imgs-l{margin-right:10px;}
.grace-list-imgs-r{margin-left:10px;}
/* 选项卡 */
.grace-tab{padding:0;}
.grace-tab-title{white-space:nowrap; text-align:center; background:#FFFFFF;}
.grace-tab-title view{width:auto; padding:0 12px; margin:0 8px; line-height:42px; display: inline-block; text-align:center; border-bottom:2px solid #FFFFFF; font-size:30rpx;}
.grace-tab-title view:first-child{margin-left: 0;}
.grace-tab-title view:last-child{margin-right: 0;}
.grace-tab-current{border-bottom:4rpx solid #00C777 !important; color:#00C777;}
.grace-tab-swiper{width:100%; height:350rpx; padding:0;}
.grace-tab-swiper swiper-item{width:100%;}
.grace-tab-swiper swiper-item navigator{line-height:70rpx; width:100%; display:block;}
.grace-tab-swiper-full{width:100%; height:auto;}
.grace-tab-swiper-full swiper-item{width:100%;}
.grace-tab-swiper-full scroll-view{width:100%; height:100%;}
/* 折叠面板 */
.grace-accordion{background:#FFFFFF;}
.grace-accordion-items{overflow:hidden; border-bottom:1px solid #F2F3F4;}
.grace-accordion-items:last-child{border:none;}
.grace-accordion-title{display:-webkit-box;display:-webkit-flex;display:flex; -webkit-flex-wrap:nowrap; flex-wrap:nowrap; width:92%; -webkit-box-pack:justify; -webkit-justify-content:space-between; justify-content:space-between; padding:0 4% 0 4%; line-height:88rpx; height:88rpx; overflow:hidden; font-size:32rpx;}
.grace-accordion .grace-current{background:#F2F3F4;}
.grace-accordion-body{overflow:hidden;}
/* 标题及更多 */
.grace-title{padding:10rpx 0; line-height:1.8em;}
.grace-title .grace-text-small{color:#888;}
.grace-more-bottom{font-size:24rpx; text-align:center; -webkit-box-pack:center; -webkit-justify-content:center; justify-content:center; padding:15rpx 0; line-height:40rpx;}
.grace-more-bottom text{font-size:24rpx;}
.grace-more-r{display:block; width:auto; -webkit-flex-shrink:0; flex-shrink:0; font-size:22rpx;}
.grace-more-r text{font-size:22rpx;}
/* 横向广告组 */
.grace-box-banner{padding:20rpx 0; display:-webkit-box; display:-webkit-flex; display:flex; -webkit-flex-wrap:nowrap; flex-wrap:nowrap; overflow:hidden; background:#FFF;}
.grace-box-banner .garce-items{width:25%; border-right:1px solid #F1F2F3; -webkit-box-pack:center; -webkit-justify-content:center; justify-content:center; line-height:1.4em; text-align:center；}
.grace-box-banner .garce-items:last-child{border:none;}
.grace-box-banner .garce-items view{-webkit-box-pack:center;-webkit-justify-content:center;justify-content:center; text-align:center;}
.grace-box-banner .garce-items text text{font-size:22rupx; color:#666;}
.grace-box-banner .line1{font-size:36rpx; line-height:60rpx; overflow:hidden;}
.grace-box-banner .line1 text{font-size:26rpx; color:#666; line-height:65rpx; margin-left:5rpx;}
.grace-box-banner .line2{font-size:26rpx; color:#666; line-height:32rpx;}
/* 可选标签 */
.grace-select-tips{padding:10rpx 0;}
.grace-select-tips checkbox-group, .grace-select-tips radio-group{width:100%; display:-webkit-box; display:-webkit-flex; display:flex; -webkit-flex-wrap:wrap; flex-wrap:wrap;}
.grace-select-tips label{display:block; width:auto; overflow:hidden; padding:15rpx 22rpx; height:30rpx; line-height:30rpx; margin:6px; background:#F6F7F8; font-size:26rpx; border-radius:5rpx;}
.grace-select-tips label checkbox, .grace-select-tips label radio{display:none;}
.grace-checked{background:#00C777 !important; color:#FFFFFF;}
/* 底部导航 */
.grace-footer{display:-webkit-box;display:-webkit-flex;display:flex; position:fixed; z-index:2; left:0; bottom:0; background:#FFFFFF; width:100%; height:90rpx; -webkit-flex-wrap:nowrap; flex-wrap:nowrap; overflow:hidden; box-shadow:1px 1px 6px #888;}
.grace-footer .grace-input{width:100%; margin:15rpx 20rpx; padding:0 15rpx; background:#F4F5F6; height:60rpx; border-radius:60rpx; -webkit-flex-wrap:nowrap; flex-wrap:nowrap; display:-webkit-box; display:-webkit-flex; display:flex;}
.grace-input-icon{width:60rpx; height:60rpx; line-height:60rpx; font-size:30rpx; color:#000000; -webkit-box-pack:center; -webkit-justify-content:center; justify-content:center; -webkit-flex-shrink:0; flex-shrink:0;}
.grace-footer .grace-input input{width:100%; padding:10rpx 15rpx; height:40rpx; line-height:40rpx; font-size:26rpx; background:none; border:0;}
.grace-footer .icons{width:70rpx; height:60rpx; margin:15rpx 8rpx; text-align:center; line-height:60rpx; -webkit-flex-shrink:0; flex-shrink:0; font-size:50rpx;}
.grace-footer .grace-items{width:auto; line-height:90rpx; -webkit-flex-shrink:0; flex-shrink:0;}
.grace-footer button{width:100%; border:0; border-radius:0; height:90rpx; line-height:90rpx;}
/* 表单 */
.grace-form{padding:0; width:100%;}
.grace-form form{display:block; width:100%; overflow:hidden;}
.grace-form .grace-items{display:-webkit-box;display:-webkit-flex;display:flex; border-bottom:1px solid #F2F3F4; width:100%; -webkit-flex-wrap:nowrap; flex-wrap:nowrap; -webkit-box-pack:justify; -webkit-justify-content:space-between; justify-content:space-between; padding:10rpx 0;}
.grace-form .grace-items .grace-label{width:130rpx; height:80rpx; line-height:80rpx; -webkit-flex-shrink:0; flex-shrink:0; overflow:hidden;}
.grace-form .grace-items .input{width:98%; height:40rpx; line-height:40rpx; background:none; -webkit-flex-shrink:1; flex-shrink:1; border:0; text-align:right; padding:20rpx 2%; margin-left:40rpx;}
.grace-form-r{width:100%; padding:0 10rpx; line-height:80rpx; display:block; overflow:hidden; -webkit-flex-shrink:1; flex-shrink:1; margin-left:40rpx; text-align:right;}
.grace-form picker{width:100%; height:80rpx; line-height:80rpx; background:none; border:0; text-align:right;}
.grace-form switch{margin-top:10rpx;}
.grace-form .grace-items picker text{-webkit-box-pack:right;-webkit-justify-content:right;justify-content:right; line-height:80rpx; font-size:28rpx;}
.grace-form .grace-items picker text:after{content:"\E601"; padding-left:10rpx; color:#888; font-family:"grace-iconfont" !important; font-size:30rpx;}
.grace-form textarea{width:98%; height:100rpx; line-height:2em; background:none; border:0; padding:8rpx 2%; font-size:28rpx;}
.grace-label-x{width:100%; padding:12rpx 0; -webkit-flex-shrink:1; flex-shrink:1; margin-left:40rpx;}
.grace-label-x label{margin:10rpx; font-size:28rpx;}
.grace-label-y{width:100%; padding:12rpx 0; -webkit-flex-shrink:1; flex-shrink:1; margin-left:40rpx;}
.grace-form radio-group, .grace-form checkbox-group{width:100%; display:-webkit-box; display:-webkit-flex; display:flex; -webkit-flex-wrap:wrap; flex-wrap:wrap;}
.grace-label-y label{margin:8px 0; font-size:28rpx; width:100%;}
.grace-items-wbg{background:#FFF; border:0 !important; border-radius:8rpx; padding:3rpx 0 !important;}
.grace-items-wbg input{text-align:left !important;}
.grace-login-three{display:-webkit-box;display:-webkit-flex;display:flex; -webkit-box-pack:center; -webkit-justify-content:center; justify-content:center; -webkit-flex-wrap:nowrap; flex-wrap:nowrap;}
.grace-login-three view{width:44px; height:44px; line-height:44px; font-size:36px; color:#FFF; text-align:center; margin:8px;}
/* 遮罩层 */
.grace-mask{background:rgba(0, 0, 0, 0.6); position:fixed; width:100%; height:100%; left:0; top:0; z-index:1;}
/* steps */
.grace-steps{padding:20rpx 0; background:#FFF; display:-webkit-box; display:-webkit-flex; display:flex; -webkit-flex-wrap:nowrap; flex-wrap:nowrap;}
.grace-steps view{display:-webkit-box;display:-webkit-flex;display:flex; width:100%;}
.grace-steps .step{width:100%; margin:0 5px; display:-webkit-box; display:-webkit-flex; display:flex; -webkit-flex-wrap:nowrap; flex-wrap:nowrap;}
.grace-steps .step:last-child{display:-webkit-inline-box;display:-webkit-inline-flex;display:inline-flex; -webkit-flex-shrink:0; flex-shrink:0; width:auto;}
.grace-steps .step-circle{width:50rpx; height:50rpx; border-radius:50rpx; background:#F1F1F3; -webkit-box-pack:center; -webkit-justify-content:center; justify-content:center; line-height:50rpx; -webkit-flex-shrink:0; flex-shrink:0; margin-right:15rpx; color:#666; font-size:28rpx;}
.grace-steps .step-content{width:100%; height:22rpx; border-bottom:1px solid #F1F1F3;}
.grace-steps .step-title{line-height:50rpx; height:50rpx; background:#FFFFFF; width:auto; padding-right:12rpx;}
.grace-steps .current .step-circle{background:#00B26A; color:#FFFFFF;}
.grace-steps .current .step-content{border-color:#00B26A;}
.grace-steps .current .step-title{color:#00B26A;}
/* 评论列表 */
.grace-comment{padding:5rpx 0;}
.grace-comment-list{display:-webkit-box;display:-webkit-flex;display:flex; -webkit-flex-wrap:nowrap; flex-wrap:nowrap; padding:10rpx 0; margin:10rpx 0;}
.grace-comment-face{width:70rpx; height:70rpx; border-radius:100%; margin-right:20rpx; -webkit-flex-shrink:0; flex-shrink:0; overflow:hidden;}
.grace-comment-face image{width:100%; border-radius:100%;}
.grace-comment-body{width:100%;}
.grace-comment-top{display:-webkit-box;display:-webkit-flex;display:flex; -webkit-flex-wrap:nowrap; flex-wrap:nowrap; line-height:1.5em; -webkit-box-pack:justify; -webkit-justify-content:space-between; justify-content:space-between;}
.grace-comment-top text{color:#0A98D5; font-size:24rpx;}
.grace-comment-top text:last-child{color:#666666;}
.grace-comment-date{display:-webkit-box;display:-webkit-flex;display:flex; -webkit-flex-wrap:nowrap; flex-wrap:nowrap; line-height:1.5em; -webkit-box-pack:justify; -webkit-justify-content:space-between; justify-content:space-between;}
.grace-comment-date text{color:#666666; font-size:24rpx;}
.grace-comment-content{line-height:1.6em; font-size:28rpx; padding:8rpx 0;}
.grace-comment-zan{color:#0A98D5 !important;}
.grace-comment-replay-btn{background:#F4F5F6; font-size:24rpx; padding:8rpx 15rpx; border-radius:30rpx; color:#333 !important; margin:0 10rpx;}
.grace-comment-imgs{display:-webkit-box;display:-webkit-flex;display:flex; padding:8rpx 0; -webkit-flex-wrap:wrap; flex-wrap:wrap;}
.grace-comment-imgs .imgs{width:31.3% !important; max-height:90px; margin:5px 1%; overflow:hidden;}
.grace-comment-imgs .imgs image{width:100%;}
/* 搜索 */
.grace-search{width:100%; overflow:hidden; display:-webkit-box; display:-webkit-flex; display:flex; -webkit-flex-wrap:nowrap; flex-wrap:nowrap;}
